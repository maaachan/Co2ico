 <main>
   <div class="container">
     <h5>フォロワー一覧</h5>
       <% if @users.exists? %>
       <% @users.each do |user| %>
         <%= link_to  user.name,user_path(user) %>
         <%= image_tag user.get_image, class: "rounded-circle", size: "100x100" %>
       <% end %>
       <% end %>
       <%#= link_to "戻る", user_path(@user.id), class: "button" %>
   </div>
 </main>