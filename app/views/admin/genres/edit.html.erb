<main>
  <h3>ジャンル編集</h3>
    <table class="genres">
      <thead class="thead">
      </thead>
      <tbody>
      <%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch do |f| %>
        <tr><%= f.text_field :name %></tr>
        <tr><%= f.submit '変更を保存', class: 'button' %></tr>
      <% end %>
    </tbody>
    </table>
</main>  
